<?xml version="1.0" encoding="UTF-8"?>
<widget  xmlns     = 'http://www.w3.org/ns/widgets'
        xmlns:gap = 'http://phonegap.com/ns/1.0'
		id="movil.ABC" version="1.0">

	<name>abc</name>
	<description>abc</description>
	<author>SMMM</author>	<gap:platforms>
		<gap:platform name="ios" />
		<gap:platform name="android" />
		<gap:platform name="webos" />
		<gap:platform name="symbian.wrt" />
		<gap:platform name="blackberry" project="widgets"/>
	</gap:platforms>	

	<icon src="abclogo.png" width="128" height="128" gap:role="default" />	
	<icon src="abclogo57.png" width="57" height="57" gap:role="default" />	
	<icon src="abclogo72.png" width="72" height="72" gap:role="default" />	
	<preference name="permissions" value="none"/>
	<preference name="phonegap-version" value="2.9.0" />
	<access origin="*" />
	<preference name="orientation" value="portrait" />
	<preference name="fullscreen" value="true" />
	<preference name="target-device" value="universal" />
	<!--<feature name="http://api.phonegap.com/1.0/network"/>-->	

	<gap:splash src="splash.jpg" width="768" height="1024" />

	<!-- Optional splash screen image, e.g.: -->
	<!-- <gap:splash src="splash.png" width="1024" height="768" /> -->

</widget>